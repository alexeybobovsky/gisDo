<link 	rel="stylesheet" type="text/css" href="/src/design/fancybox/jquery.fancybox.css?v=2.1.2" media="screen" /><link 	rel="stylesheet" type="text/css" href="/src/design/fancybox/jquery.fancybox-thumbs.css?v=1.0.7" /><script src="http://api-maps.yandex.ru/2.0/?load=package.standard,&mode=release&lang=ru-RU" type="text/javascript"></script><script src="/includes/JS/fs/mapYMmini.js" type="text/javascript"></script><script type="text/javascript" src="/includes/fancybox/jquery.fancybox.pack.js"></script><script type="text/javascript" src="/includes/fancybox/jquery.fancybox-thumbs.js?v=1.0.7"></script><script type="text/javascript">var fbMapCurObject = 0;{literal}$(document).ready(function() {$(".fbScheme").css({'border-bottom':'1px dotted #396398'});$(".fbScheme").hover(	function () {			$(this).css({'text-decoration':'none'});		  },	function () {			$(this).css({'text-decoration':'none'});		  } );  		  {/literal}{literal}$('.imgLink').fancybox({				wrapCSS    : 'fancybox-custom',				closeClick : true,				openEffect : 'none',				helpers : {					title : {						type : 'inside'					},					overlay : {						css : {							'background' : 'rgba(238,238,238,0.85)'						}					},					thumbs : {						width: 50,						height: 50					}				},				afterLoad: function() 					{					$('.fancybox-skin').bind("mouseover", function() {$('.fancybox-close').css({'top' : '-4px', 'right' : '-4px'})}); 					}							});	});</script>			{/literal}<input type="hidden" id="cityId" name="cityId" value='{$client.city.id}'><input type="hidden" id="cityName" name="cityName" value='{$client.city.name}'>	<div id="titleBar">		<div id='titleContent'>		<h2>{$title}</h2>		<div id='filtershow' class='simple' onClick='UI.togglePanel(document.getElementById("titleBar"), "filterPanel", 0, "highlightFilterStarter");'>Фильтр</div>{*		<div id='filtershow' onClick='UI.togglePanel(document.getElementById("titleBar"), "filterPanel", 0);'>Фильтр</div>*}		</div>	</div>	<div id="pageContent">									<div id='tablePropContaner'>				{if $obj.info}				<div class='tableProp'>					<div class='label'>						Описание					</div>					<div class='value highlight'>						{$obj.info}					</div>				</div>				{/if}				{if $obj.firmName}				<div class='tableProp'>					<div class='label'>						Застройщик					</div>					<div class='value'>						<a href='/list/firm/{$obj.firmId}'>{$obj.firmName}</a>					</div>				</div>				{/if}				{if $obj.podrId}				<div class='tableProp'>					<div class='label'>						Подрядчик					</div>					<div class='value'>						<a href='/list/firm/{$obj.podrId}'>{$obj.podrName}</a>					</div>				</div>				{/if}				<div class='tableProp'>					<div class='label'>						Район					</div>					<div class='value'>						{$obj.district}					</div>				</div>				<div class='tableProp'>					<div class='label'>						Адрес					</div>					<div class='value'>						<a class='fbScheme activeLink'  href='/map/construction/{$obj.id}' 								title='{$obj.name} схема проезда' 								onClick = 'showMiniMap("{$obj.type}", "{$obj.id}");  return false;'								target="_blank">												{$obj.location}							</a>					</div>				</div>								<div class='tableProp'>					<div class='label'>						Состояние стройки					</div>					<div class='value state_{$obj.stateId}'>						{$obj.state}					</div>				</div>				<div class='tableProp'>					<div class='label'>						Материал					</div>					<div class='value'>						{$obj.material}					</div>				</div>				{if $obj.zakaz}				<div class='tableProp'>					<div class='label'>						Заказчик					</div>					<div class='value'>						{$obj.zakaz}					</div>				</div>				{/if}				{if $obj.podr}				<div class='tableProp'>					<div class='label'>						Подрядчик					</div>					<div class='value'>						{$obj.podr}					</div>				</div>				{/if}				{if $obj.obj_dateStart}				<div class='tableProp'>					<div class='label'>						Начало строительства					</div>					<div class='value'>						{$obj.obj_dateStart}					</div>				</div>				{else}				<div class='tableProp'>					<div class='label'>						Начало мониторинга					</div>					<div class='value'>						{$obj.monitoringStart}					</div>				</div>				{/if}				{if $obj.obj_dateEnd}								<div class='tableProp'>					<div class='label'>						Срок сдачи					</div>					<div class='value'>						{$obj.obj_dateEnd}					</div>				</div>				{else}				<div class='tableProp'>					<div class='label'>						Окончание мониторинга					</div>					<div class='value'>						{$obj.monitoringEnd}					</div>				</div>				{/if}				{if $obj.obj.obj_www}												<div class='tableProp'>					<div class='label'>						Сайт					</div>					<div class='value'>						<a href='http://{$obj.obj.obj_www}' target='_blank'>{$obj.obj.obj_www}</a>					</div>				</div>				{/if}				{if $obj.sales}												<div class='tableProp'>					<div class='label'>						Отдел продаж					</div>					<div class='value'>						{$obj.sales}					</div>				</div>				{/if}				{if $obj.obj.projDoc}												<div class='tableProp'>					<div class='label'>						Проектная декларация					</div>					<div class='value'>						<a href='#'>{$obj.obj.projDoc}</a>					</div>				</div>				{/if}			</div>			<div id='fotoBig'>				<img  alt="{$obj.name}" width='400' src='{$obj.imgMain|replace:"size":"640"}'>			</div>			<div id='infoImageHolder'>							<div id='fotoSheme'>					<h3>Схема фотосъемки</h3>	{section name=img loop=$obj.img.fotoScheme}					<a  class='imgLink ' href='{$obj.img.fotoScheme[img]|replace:"size":"1024"}' 							rel="gallery_sheme" title="Схема съёмки объекта  {$title} город {$client.city.name}"target="_blank">						<img src='{$obj.img.fotoScheme[img]|replace:"size":"150"}'>					</a>	{/section}				</div>								<div id='fotoInfo'>					<h3>Информационные доски</h3>	{section name=img loop=$obj.img.fotoInfo}					<a  class='imgLink ' href='{$obj.img.fotoInfo[img]|replace:"size":"1024"}' 							rel="gallery_info" title="Фото информационной доски объекта  {$title} город {$client.city.name}"target="_blank">						<img  src='{$obj.img.fotoInfo[img]|replace:"size":"150"}'>					</a>	{/section}				</div>							</div>		{if $client.isMng}		<div id='manageHolder'>		<span class="mngAct" id="af"  onClick="showEdtBox('{$client.mngAct.af}{$obj.id}', 0)"><nobr>съёмка</nobr></span>		<span class="mngAct" id="oe"  onClick="showEdtBox('{$client.mngAct.oe}{$obj.type}/{$obj.id}', 0)"><nobr>изменить</nobr></span>		<span class="mngAct" id="od"  onClick="delObj('{$client.mngAct.od}', 0)"><nobr>удалить</nobr></span>		</div>		{/if}			<div id='FotoSetHolder'>				{section name=foto loop=$obj.foto}		{if $smarty.section.foto.first or $obj.foto[$smarty.section.foto.index_prev].year!= $obj.foto[foto].year}				<div id='fotoSetYearHold'>					<h3  id='label_{$obj.foto[foto].yearDigit}' {if $obj.foto[foto].yearDigit != $obj.lastYear}class='more'{/if} >{$obj.foto[foto].year}&nbsp;&nbsp;<span id='show_{$obj.foto[foto].yearDigit}' class='activeLink' onClick='showFotoSet("{$obj.foto[foto].yearDigit}", "{$obj.foto[foto].capture}")'>{if $obj.foto[foto].yearDigit == $obj.lastYear}скрыть{else}показать{/if}</span></h3>					<div class='fotoSet' ></div>				{/if}							{if $smarty.section.foto.first or $obj.foto[$smarty.section.foto.index_prev].capture!= $obj.foto[foto].capture}					<div id='fotoSet_{$obj.foto[foto].capture}' class='fotoSet fotoSet_{$obj.foto[foto].yearDigit}' {if $obj.foto[foto].yearDigit != $obj.lastYear}style='display:none;'{/if}>						<h4>{$obj.foto[foto].date}</h4>		{/if}						<a  class='imgLink ' href='{$obj.foto[foto].src|replace:"size":"1024"}' 								rel="gallery_foto_{$obj.foto[foto].yearDigit}" title="Фото {$obj.foto[foto].date} c позиции {$obj.foto[foto].position}. Объект  {$title} город {$client.city.name}" target="_blank">							<img  src='{$obj.foto[foto].src|replace:"size":"150"}'>						</a>		{if $smarty.section.foto.last or $obj.foto[$smarty.section.foto.index_next].capture!= $obj.foto[foto].capture}					</div>		{/if}		{if $smarty.section.foto.last or $obj.foto[$smarty.section.foto.index_next].year!= $obj.foto[foto].year}				</div>						{/if}	{/section}			</div>	<div id='empty'></div>			</div>