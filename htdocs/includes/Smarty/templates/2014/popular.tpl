<link 	rel="stylesheet" type="text/css" href="/src/design/fancybox/jquery.fancybox.css?v=2.1.2" media="screen" /><link 	rel="stylesheet" type="text/css" href="/src/design/fancybox/jquery.fancybox-thumbs.css?v=1.0.7" /><link 	rel="stylesheet" type="text/css" href="/includes/jwysiwyg/jquery.wysiwyg.css" /><script src="http://api-maps.yandex.ru/2.0/?load=package.standard,&mode=release&lang=ru-RU" type="text/javascript"></script><script src="/includes/JS/fs/mapYMmini.js" type="text/javascript"></script><script type="text/javascript" src="/includes/fancybox/jquery.fancybox.pack.js"></script><script type="text/javascript" src="/includes/fancybox/jquery.fancybox-thumbs.js?v=1.0.7"></script><script src="/includes/JS/fs/userMessage.js" type="text/javascript"></script><script type="text/javascript" src="/includes/jwysiwyg/jquery.wysiwyg.js"></script><script type="text/javascript" src="/includes/jwysiwyg/controls/wysiwyg.link.js"></script><script type="text/javascript" src="/includes/jwysiwyg/plugins/wysiwyg.i18n.js"></script> <script type="text/javascript" src="/includes/jwysiwyg/i18n/lang.ru.js"></script><script type="text/javascript">var fbMapCurObject = 0;var activeSufix = '';var commentBoxActive = false;var errorBoxActive = false;{literal}$(document).ready(function() {$(".fbScheme").css({'border-bottom':'1px dotted #396398'});$(".fbScheme").hover(	function () {			$(this).css({'text-decoration':'none'});		  },	function () {			$(this).css({'text-decoration':'none'});		  } );  		  {/literal}{literal}$('.imgLink').fancybox({				wrapCSS    : 'fancybox-custom',				closeClick : true,				openEffect : 'none',				helpers : {					title : {						type : 'inside'					},					overlay : {						css : {							'background' : 'rgba(238,238,238,0.85)'						}					},					thumbs : {						width: 50,						height: 50					}				},				afterLoad: function() 					{					$('.fancybox-skin').bind("mouseover", function() {$('.fancybox-close').css({'top' : '-4px', 'right' : '-4px'})}); 					}							});						{/literal}{if $client.isMng}{literal}		$('[id ^= mesState]').bind("click", function() 	{mesToggle(this)});			$('[id ^= mesDel]').bind("click", function() 	{mesDel(this)});			{/literal}{/if}{literal}							});</script>			{/literal}	<div id="titleBar">		<div id="titleContent">			<div id="string">				<nobr><h1 title='{$title}' >{if $h1}{$h1}{else}{$title}{/if}&nbsp;</h1></nobr>				<div id='titleBlur' title='{$title}'></div>			</div>			{if $objNumStr}			<span id='pageTitleMore' >({$objNumStr})</span>			{/if}		</div>	</div>		<div id="pageContent">					<div id='tablePropContaner'>				<div class='tableProp'>					<div class='label'>						Район					</div>					<div class='value'>						{$main.district}					</div>				</div>				<div class='tableProp'>					<div class='label'>						Адрес					</div>					<div class='value'>						<a class='fbScheme activeLink'  href='/map/construction/{$main.id}' 								title='{$main.name} схема проезда' 								onClick = 'showMiniMap("construction", "{$main.id}");  return false;'								target="_blank">												{$main.location}							</a>					</div>				</div>										{if $main.firmList}												<div class='tableProp'>					<div class='label'>						Участники строительства					</div>					<div class='value'>					{section name=firm loop=$main.firmList}						<a href='/list/firm/{$main.firmList[firm].id}'>{$main.firmList[firm].name}</a> </br>					{/section}					</div>				</div>				{/if}				{if $main.sales}												<div class='tableProp'>					<div class='label'>						Отдел продаж					</div>					<div class='value'>						{$main.sales}					</div>				</div>				{/if}				{if $main.www}												<div class='tableProp'>					<div class='label'>						Сайт					</div>					<div class='value'>						<a  rel="nofollow" href='http://{$main.www}' target='_blank'>{$main.www}</a>					</div>				</div>				{/if}			</div>			<div id='fotoBig'>	{assign var="fotoRender" value=$main.renderList}	{*$fotoRender[0]*}	{if $fotoRender}		{section name=img loop=$fotoRender}			{if $smarty.section.img.first}				<div class='renderPad'>					<a  class='renderFull' onClick='showRender();  return false;' href='{$fotoRender[img]|replace:"size":"1024"}' 							 title='Рендер объекта  {$main.name} город {$client.city.name}' target="_blank">						<img alt='' id='renderFull' class= 'prew' src='{$fotoRender[img]|replace:"size":"640"}' />					</a>				</div>			{/if}			{if $smarty.section.img.first && $smarty.section.img.last}			{else}					<a  class='renderIco'  onClick='previewRender(this);  return false;' 							href='{$fotoRender[img]|replace:"size":"1024"}' 							title='Рендер объекта {$main.name} город {$client.city.name}' target="_blank">						<img alt='' class='icon  {if $smarty.section.img.first}iconSelected{/if}' src='{$fotoRender[img]|replace:"size":"45"}' />										</a>			{/if}		{/section}				<img  alt='' id='load' src='/src/design/main/_.gif'/>						{/if}			</div>			<div id="infoTextHolder">					{if $main.infoPrev}				<div id="prew"><p>{$main.infoPrev}</p></div>				<div id="other">{$main.infoMore}</div>				<div style='text-align:right;'>					<span id='more' class='activeLink' onClick='toggleFulText()'>читать полностью >></span></div>				{else}				<div id="prew"><p>{$main.info}				</div>								{/if}			</div>		<div id="listContaner">			{*			<h2>Очереди строительства</h2>*}			<div class='bigLabel'>Очереди строительства</div>	{section name=list loop=$main.list}			<div class="listItem">					<div class="itemTop">					 <div class="itemNum"><p>{*$smarty.section.list.iteration*}{$main.list[list].number}.</p>					 </div>					 <div class="itemName">						<a  rel="nofollow" title="{$main.list[list].name} в городе Иркутск" href="/list/construction/{$main.list[list].id}">{$main.list[list].name}</a>					 </div>					<div title="Средняя оценка - " class="itemRate">					&nbsp;					</div>					 <div class="itemMore">						 <a  rel="nofollow" class="оrganization_events_more" title="Подробнее о {$main.list[list].name} в городе Иркутск" href="/list/construction/{$main.list[list].id}"> Подробнее&nbsp;»</a>					 </div>                       				</div>                    				<div class="itemMiddle">   					<div class="itemIcon">						<img alt="Фото {$main.list[list].name} в городе Иркутск" title="Фото {$main.list[list].name} в городе Иркутск" class="itemIconImg" src="{$main.list[list].icon|replace:"size":"150"}" />                             					</div>                             					<div class="itemInfo">						<div class='tablePropList'>							<div class='label'>								Адрес							</div>							<div class='value'>								<a target="_blank" 									onclick="showMiniMap('construction', '{$main.list[list].id}');  return false;" 									title="{$main.list[list].name} схема проезда" 									href="/map/construction/{$main.list[list].id}" 									class="fbScheme activeLink" >{$main.list[list].location}</a>							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Район							</div>							<div class='value'>								{$main.list[list].district}							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Состояние стройки							</div>							<div class='value state_{$main.list[list].stateId}'>								{$main.list[list].state}							</div>						</div>						{*<div class='tablePropList'>							<div class='label'>								Роль в проекте							</div>							<div class='value'>								{$list[list].role}							</div>						</div>*}						<div class='tablePropList'>							<div class='label'>								Последнее фото:							</div>							<div class='value'>								 {$main.list[list].fotoLastDate}							</div>						</div>										 </div>                                										</div>                    			</div> 	{/section}					 		</div>					<div id='empty'></div>				</div>