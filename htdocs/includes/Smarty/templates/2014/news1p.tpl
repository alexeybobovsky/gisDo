				{if $naviKeyword}<script type="text/javascript">{literal}$(document).ready(function() {		$("#keywordPanel").css({'display':'none'});				});</script>			{/literal}				<div id="keywordPanel" >        	<div id="keywordPanelList">		<ul class="keywordPanelList">{section name=lay loop=$naviKeyword}{if $smarty.section.lay.index != 0 && $smarty.section.lay.index%15 == 0}</ul><ul class="keywordPanelList">{/if}		<li id='nameLayer_{$naviKeyword[lay].catId}'  title='{$naviKeyword[lay].cat_name}'>				<a href="/news/key/{$naviKeyword[lay].cat_name}" title="">{$naviKeyword[lay].cat_name}</a> 				</li>{/section}							</ul>	</div>								   </div>				{/if}  {*<div id="titleBar">	<div id='titleContent'>		<h2 id='newsTitle' >{$title}</h2>	<span>{$objNumStr}</span>	</div></div>*}	<div id="titleBar">		<div id="titleContent">			<div id="string">				<nobr><h1 title='{$title}' >{$title}&nbsp;</h1></nobr>				<div id='titleBlur' title='{$title}'></div>			</div>			{if $objNumStr}			<span id='pageTitleMore' >({$objNumStr})</span>			{/if}		</div>	</div><div id="pageContent">		{if $navi}			<div id="navigation">			<ul class="navigation">				{section name=nav loop=$navi} 				{if $navi[nav].src}				<li><a href="{$navi[nav].src}" title="{$navi[nav].title}" >{$navi[nav].caption}</a></li>				{elseif $navi[nav].actionLink}				<li><p  class="activeLink" id = 'newsCatSelect'   title="{$navi[nav].title}" onClick="UI.togglePanel(this, 'keywordPanel', 0, '')" >{$navi[nav].caption}</p></li>				{else}				<li><p  title="{$navi[nav].title}" >{$navi[nav].caption}</p></li>				{/if}				{if $navi[$smarty.section.nav.index_next].caption}				<li><p>|</p></li>				{/if}				{/section}			</ul>		</div>  	{/if}	{section name=news loop=$newsList}                               	   <div id="news_body">                   		                        <div id="news_top">                             <div id="news_header">                             	<h2 class="news_header" ><a href="{$newsList[news].link}" title="">{$newsList[news].news_name}</a></h2>                             </div>                             <div id="news_date">                             	<p class="news_date">{$newsList[news].comm_date_ru}</p>                             </div>                                               </div>                                        	<div id="news_middle"> {if $newsList[news].news_img != ''}                             <div id="news_img">                             	<img src="{$newsList[news].news_img}" class="news_img" title="{$newsList[news].news_name}" alt="" />                                                          </div>                                 <div id="news_text">{else}                             <div id="news_text_only">{/if}                                 <p class="news_text"> {$newsList[news].news_header}                                 </p>                             	 <a  rel="nofollow" href="{$newsList[news].link}" title="Подробнее" class="news_more" /> Подробнее &raquo;</a>                             </div>                           </div>                                       <div id="news_keywords"> 					{section name=kwrd loop=$newsList[news].attr.tags}						<span> <a  rel="nofollow" href='{$properties.linkKeywordPref}{$newsList[news].attr.tags[kwrd].cat_name}'>{$newsList[news].attr.tags[kwrd].cat_name}</a></span>					{/section}				   </div>                   </div>{/section}				   			<div class='emptyH50'>																			</div></div>