<link 	rel="stylesheet" type="text/css" href="/src/design/fancybox/jquery.fancybox.css?v=2.1.2" media="screen" /><link 	rel="stylesheet" type="text/css" href="/src/design/fancybox/jquery.fancybox-thumbs.css?v=1.0.7" /><script src="http://api-maps.yandex.ru/2.0/?load=package.standard,&mode=release&lang=ru-RU" type="text/javascript"></script><script src="/includes/JS/fs/mapYMmini.js" type="text/javascript"></script><script type="text/javascript" src="/includes/fancybox/jquery.fancybox.pack.js"></script><script type="text/javascript" src="/includes/fancybox/jquery.fancybox-thumbs.js?v=1.0.7"></script><script type="text/javascript">var fbMapCurObject = 0;{literal}$(document).ready(function() {$(".fbScheme").css({'border-bottom':'1px dotted #396398'});$(".fbScheme").hover(	function () {			$(this).css({'text-decoration':'none'});		  },	function () {			$(this).css({'text-decoration':'none'});		  } );  		  {/literal}{literal}$('.imgLink').fancybox({				wrapCSS    : 'fancybox-custom',				closeClick : true,				openEffect : 'none',				helpers : {					title : {						type : 'inside'					},					overlay : {						css : {							'background' : 'rgba(238,238,238,0.85)'						}					},					thumbs : {						width: 50,						height: 50					}				},				afterLoad: function() 					{					$('.fancybox-skin').bind("mouseover", function() {$('.fancybox-close').css({'top' : '-4px', 'right' : '-4px'})}); 					}							});	});</script>			{/literal}<div	 itemscope itemtype="http://schema.org/Organization">	<div id="titleBar">		<div id="titleContent">			<div id="string">				<nobr><h1 title='{$title}' ><q  itemprop="name" >{$title}</q> &nbsp;&nbsp;  - информация об организации&nbsp;&nbsp;</h1></nobr>				<div id='titleBlur' title='{$title}'></div>			</div>		</div>	</div>{*					<div id="titleBar">						<div id="titleContent">						<h2>{$title}</h2>						</div>					</div>*}						<div id="pageContent">	{if $navi}			<div id="navigation">			<ul class="navigation">				{section name=nav loop=$navi} 				{if $navi[nav].src}				<li><a href="{$navi[nav].src}" title="{$navi[nav].title}" >{$navi[nav].caption}</a></li>				{elseif $navi[nav].actionLink}				<li><p  class="activeLink" id = 'newsCatSelect'   title="{$navi[nav].title}" onClick="UI.togglePanel(this, 'keywordPanel', 0, '')" >{$navi[nav].caption}</p></li>				{else}				<li><p  title="{$navi[nav].title}" >{$navi[nav].caption}</p></li>				{/if}				{if $navi[$smarty.section.nav.index_next].caption}				<li><p>/</p></li>				{/if}				{/section}			</ul>		</div>  	{/if}			<div id='firmContaner'>			<div class='firmInfo'>						<p itemprop="review">						{$firm.firm_info}						</p>						<div  itemprop="address"  itemscope itemtype="http://schema.org/PostalAddress" class='tableProp'>							<div class='label'>								<h2>Адрес</h2>							</div>							<div class='value'>								<a class='fbScheme activeLink'  href='/map/firm/{$firm.firm_id}' 										title='{$firm.firm_name} схема проезда' 										onClick = 'showMiniMap("{$firm.type}", "{$firm.firm_id}");  return false;'										target="_blank"  itemprop="streetAddress">														{$firm.location}									</a>							</div>						</div>						<div class='tableProp'>							<div class='label'>								<h2>Телефон</h2>							</div>							<div class='value'  itemprop="telephone">								{$firm.phone}							</div>						</div>						{if $firm.firm_www}														<div class='tableProp'>							<div class='label'>								<h2>Сайт</h2>							</div>							<div class='value'>								<a href='http://{$firm.firm_www}' target='_blank'  rel="nofollow">{$firm.firm_www}</a>							</div>						</div>						{/if}									</div>			{if $firm.firm_logo}			<div id='fotoBig'>				<img  alt="{$firm.firm_name}" src='{$firm.firm_logo|replace:"size":"400"}'>			</div>			{/if}		</div>			<div id="listContaner">						<h2>Объекты строительства</h2>	{section name=list loop=$list}			<div id="listItem1" class="listItem">					<div class="itemTop">					 <div class="itemNum"><p>{*$smarty.section.list.iteration*}{$list[list].number}.</p>					 </div>					 <div class="itemName">						<h3><a title="{$list[list].name} в городе Иркутск" href="/list/construction/{$list[list].id}">{$list[list].name}</a></h3>					 </div>					<div title="Средняя оценка - " class="itemRate">					&nbsp;					</div>					 <div class="itemMore">						 <a  rel="nofollow" class="оrganization_events_more" title="Подробнее о {$list[list].name} в городе Иркутск" href="/list/construction/{$list[list].id}"> Подробнее&nbsp;»</a>					 </div>                       				</div>                    				<div class="itemMiddle">   					<div class="itemIcon">						<img alt="Фото {$list[list].name} в городе Иркутск" title="Фото {$list[list].name} в городе Иркутск" class="itemIconImg" src="{$list[list].icon|replace:"size":"150"}">                             					</div>                             					<div class="itemInfo">						{*if $list[list].firmName}						<div class='tablePropList'>							<div class='label'>								Застройщик							</div>							<div class='value'>								<a href='/list/firm/{$list[list].firmId}'>{$list[list].firmName}</a>							</div>						</div>						{/if*}						<div class='tablePropList'>							<div class='label'>								Адрес							</div>							<div class='value'>								<a target="_blank" 									onclick="showMiniMap('construction', '{$list[list].id}');  return false;" 									title="{$list[list].name} схема проезда" 									href="/map/construction/{$list[list].id}" 									class="fbScheme activeLink" >{$list[list].location}</a>							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Район							</div>							<div class='value'>								{$list[list].district}							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Состояние стройки							</div>							<div class='value state_{$list[list].stateId}'>								{$list[list].state}							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Роль в проекте							</div>							<div class='value'>								{$list[list].role}							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Последнее фото:							</div>							<div class='value'>								 {$list[list].fotoLastDate}							</div>						</div>										 </div>                                										</div>                    			</div> 	{/section}					 		</div>	<div id='empty'></div>			</div></div>