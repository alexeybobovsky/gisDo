	<div id="titleBar">		<div id='titleContent'>		<h2>{$title}</h2>	</div>	</div>	<div id="pageContent" >								<div id="siteMap">			<ul>			{section name=list loop=$list}				{if !$list[list].lvl}				<li>					{if $list[list].link}					<h3><a  style='font-size:18px;'  href = '{$list[list].link}'>{$list[list].name}</a></h3>					{else}						{if $list[list].linkList}						<h3><a  style='font-size:18px;'  href = '{$list[list].linkList}'>{$list[list].name}</a></h3>												{/if}						{if $list[list].linkMap}						<h3><a  style='font-size:18px;'  href = '{$list[list].linkMap}'>{$list[list].name} на карте</a></h3>						{/if}					{/if}				</li>				{else}				<li style='margin-left:50px;'>					{if $list[list].link}					<h3><a   href = '{$list[list].link}'>{$list[list].name}</a></h3>					{else}						{if $list[list].linkList}						<h3><a   href = '{$list[list].linkList}'>{$list[list].name}</a></h3>												{/if}						{if $list[list].linkMap}						<h3><a  href = '{$list[list].linkMap}'>{$list[list].name} на карте</a></h3>						{/if}					{/if}				</li>				{/if}			{/section}			</ul>		</div>	<div id='empty'></div>			</div>