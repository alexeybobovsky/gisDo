<script src="http://api-maps.yandex.ru/2.0/?load=package.standard,&mode=release&lang=ru-RU" type="text/javascript"></script><script src="/includes/JS/fs/mapYMmini.js" type="text/javascript"></script>	<div id="titleBarExt">		<div id='titleContent'>			<div id="string">				<nobr><h1 title='{$title}' >{if $h1}{$h1}{else}{$title}{/if}&nbsp;&nbsp;</h1></nobr>				<div id='titleBlur' title='{$title}'></div>			</div>					{if $objCount}			<span id='pageTitleMore' >({$objCount})</span>			{/if}			<div id='otherView'><a href='{$startUrl|replace:"list":"map"}'>показать на карте</a></div>		</div>	</div>		<div id="pageContent">		{if $list != 0}		<div id="listContaner">			<!--noindex-->				<ul class="news_list">				<li class='sortLabel' >Сортировать</li>			{if $sort.curDir == 'Asc'}				<li class='sortValue sortSelected'>по возрастанию</li>			{else}				<li class='sortValue' > <a  rel="nofollow" href="{$startUrl}{$sort.paramStr}Asc{if $sort.limitStr}/limit_{$sort.curNumOnPage}~1{/if}" title="">по возрастанию</a></li>                 						{/if}				<li>/</li>			{if $sort.curDir == 'Asc'}				<li class='sortValue' > <a  rel="nofollow" href="{$startUrl}{$sort.paramStr}Desc{if $sort.limitStr}/limit_{$sort.curNumOnPage}~1{/if}" title="">по убыванию</a></li>                 			{else}				<li class='sortValue sortSelected' > по убыванию</li>                 						{/if}				<li>&nbsp;</li>				<li class='sortLabel'>Результатов на страницу</li>			{if $sort.curNumOnPage == '10'}				<li class='sortValue sortSelected'>10</li>                 				<li>/</li>				<li class='sortValue'><a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}/limit_50~1" title="">50</a></li>                 				<li>/</li>				<li class='sortValue'><a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}" title="">Все</a></li>                 			{elseif $sort.curNumOnPage == '50' }				<li class='sortValue'><a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}/limit_10~1" title="">10</a></li>                 				<li>/</li>				<li class='sortValue sortSelected'>50</li>                 							<li>/</li>				<li class='sortValue'><a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}" title="">Все</a></li>                 			{else}				<li class='sortValue'><a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}/limit_10~1" title="">10</a></li>                 				<li>/</li>				<li class='sortValue'><a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}/limit_50~1" title="">50</a></li>                 				<li>/</li>				<li class='sortValue sortSelected'>Все</li>                 						{/if}			</ul>										<!--/noindex-->		{section name=list loop=$list}			<div id="listItem1" class="listItem">					<div class="itemTop">					 <div class="itemNum"><p>{*$smarty.section.list.iteration*}{$list[list].number}.</p>					 </div>					 <div class="itemName">						<a title="{$list[list].name} в городе Иркутск" href="/list/construction/{$list[list].id}">{$list[list].name}</a>					 </div>					<div title="Средняя оценка - " class="itemRate">					&nbsp;					</div>					 <div class="itemMore">						 <a  rel="nofollow" class="оrganization_events_more" title="Подробнее о {$list[list].name} в городе Иркутск" href="/list/construction/{$list[list].id}"> Подробнее&nbsp;»</a>					 </div>                       				</div>                    				<div class="itemMiddle">   					<div class="itemIcon">						<img alt="Фото {$list[list].name} в городе Иркутск" title="Фото {$list[list].name} в городе Иркутск" class="itemIconImg" src="{$list[list].icon|replace:"size":"150"}" />                             					</div>                             					<div class="itemInfo">						{if $list[list].firmName}						<div class='tablePropList'>							<div class='label'>								Застройщик							</div>							<div class='value'>								<a href='/list/firm/{$list[list].firmId}'>{$list[list].firmName}</a>							</div>						</div>						{/if}						{if $list[list].podrId}						<div class='tablePropList'>							<div class='label'>								Подрядчик							</div>							<div class='value'>								<a href='/list/firm/{$list[list].podrId}'>{$list[list].podrName}</a>							</div>						</div>						{/if}						<div class='tablePropList'>							<div class='label'>								Адрес							</div>							<div class='value'>								<a target="_blank" 									onclick="showMiniMap('{$list[list].type}', '{$list[list].id}');  return false;" 									title="{$list[list].name} схема проезда" 									href="/map/construction/{$list[list].id}" 									class="fbScheme activeLink" >{$list[list].location}</a>							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Район							</div>							<div class='value'>								{$list[list].district}							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Состояние стройки							</div>							<div class='value state_{$list[list].stateId}'>								{$list[list].state}							</div>						</div>						<div class='tablePropList'>							<div class='label'>								Последнее фото:							</div>							<div class='value'>								 {$list[list].fotoLastDate}							</div>						</div>						{if $list[list].objApartmentCnt}						<div class='tablePropList'>							<div class='label'>								Количество квартир							</div>							<div class='value'>								{$list[list].objApartmentCnt}							</div>						</div>						{/if}						{if $list[list].fotoControlDate}						<div class='tablePropList'>							<div class='label'>								{$list[list].fotoControlDateText}							</div>							<div class='value'>								{$list[list].fotoControlDate}							</div>						</div>						{/if}										 </div>                                										</div>                    			</div> 	{/section}						<div class='emptyH50'></div>{if $sort.pagerFullCnt > 1}  		<!--noindex-->	 							<ul class="news_list">					<li class='sortValue'>		{if $sort.prewPage}						<a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}/limit_{$sort.curNumOnPage}~{$sort.prewPage}"title="назад" ><<</a>		{else}						<<		{/if}					</li>					<li class='sortValue'>&nbsp;</li>		{section name=pager loop=$sort.pagerFullCnt} 			{if $smarty.section.pager.iteration == $sort.curPage}					<li class='sortValue sortSelected'>{$smarty.section.pager.iteration}</li>			{else}					<li class='sortValue'>						<a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}/limit_{$sort.curNumOnPage}~{$smarty.section.pager.iteration}" title="перейти на страницу {$smarty.section.pager.iteration}" alt="">{$smarty.section.pager.iteration}</a>					</li>			{/if}		{/section}					<li class='sortValue'>&nbsp;</li>					<li class='sortValue'>		{if $sort.nextPage}						<a  rel="nofollow" href="{$startUrl}{$sort.paramStr}{$sort.curDir}/limit_{$sort.curNumOnPage}~{$sort.nextPage}"title="вперёд" >>></a>		{else}						>>		{/if}					</li>				</ul>		<!--/noindex-->	{/if}							</div>{else}	<div id="message">			{$noFoundMessage}	</div>	{/if}			<div id='empty'></div>			</div>