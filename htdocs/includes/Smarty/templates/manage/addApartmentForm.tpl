<link 	rel="stylesheet" href="/src/design/chosen/chosen.css" type="text/css" />	<link 	rel="stylesheet" href="/src/design/beautyForms.css" type="text/css" /><link 	rel="stylesheet" href="/src/design/autoComlete.css" type="text/css" /><link  	rel="stylesheet" href="/src/design/fileuploader.css" type="text/css"/><script type="text/javascript" src="/includes/jquery/jquery.js"></script><script type="text/javascript" src="/includes/chosen/chosen.jquery.js"></script>	<script type="text/javascript" src="/includes/slidinglabels/slidinglabels.js"></script><script type="text/javascript" src="/includes/jquery/jquery.autocomplete.js"></script><SCRIPT type="text/javascript" src="/includes/jquery/fileuploader.js" ></script><SCRIPT type="text/javascript" src="/includes/JS/fs/admAction.js" ></script><SCRIPT type="text/javascript" src="/includes/FCKeditor/fckeditor.js" ></script><form action="{$action}" method="post" id="info" enctype="multipart/form-data">  <div id="container">	 <!--h1>Добавление павильона к рынку <strong>автоград</strong>.</h1-->		<input type="hidden" id="constrId" 		name="constrId" 	value='{if $obj.ap_objId}{$obj.ap_objId}{/if}'>				<input type="hidden" id="firmId" 		name="firmId" 		value='{if $obj.firmId}{$obj.firmId}{/if}'>		        <input type="hidden" id="objType" 		name="objType" 		value='{$objType}'>		        <input type="hidden" id="cityId" 		name="cityId" 		value='{$obj.cityId}'>		        <input type="hidden" id="objId" 		name="objId" 		value='{$obj.ap_id}'>		        <input type="hidden" id="CMP_firmId" 	name="CMP_firmId" 	value="{$obj.firmId}">		        <input type="hidden" id="CMP_constrId" 	name="CMP_constrId" value="{$obj.ap_objId}">		        <input type="hidden" id="CMP_blockSection" 	name="CMP_blockSection" 	value="{$obj.ap_blockSection}">		        <input type="hidden" id="CMP_roomNum" 	name="CMP_roomNum" 	value="{$obj.ap_roomNum}">		        <input type="hidden" id="CMP_area" 		name="CMP_area" 	value="{$obj.ap_area}">		        <input type="hidden" id="CMP_floor" 	name="CMP_floor" 	value="{$obj.ap_floor}">		        <input type="hidden" id="CMP_finish" 	name="CMP_finish" 	value="{$obj.ap_finish}">		        <input type="hidden" id="CMP_price" 	name="CMP_price" 	value="{$obj.ap_price}">		        <input type="hidden" id="CMP_sold" 		name="CMP_sold" 	value="{$obj.ap_sold}">					<h2>Квартиры</h2>	{if $obj.ap_id}		<h3>Изменить квартиру</h3>  	{else}		<h3>Добавить новую квартиру</h3>  	{/if}	<div class='side-by-side clearfix'>	</div>	{if !$obj.ap_id}	 	<div class="slider">	        <em>Клонирование</em>        	</div>    <div id="firmName-wrap" class="slider" style = 'float: left;'>        <label for="clone">Количество объектов</label>        <input type="text" id="clone" name="clone" value=''>    </div>	<span style = 'margin-left: 2px; padding: 0px;'><small >Не заполнять при добавлении</br>&nbsp;единичного объекта</small></span>	{/if}	<div id="firmName-wrap" class="slider">        <label for="constrName">Название стройки</label>        <input type="text" id="constrName" name="constrName" value={if $obj.obj_name}'{$obj.obj_name}'{else}''{/if}>    </div><!--/#name-wrap-->    <div id="firmName-wrap" class="slider">        <label for="VAL_blockSection">Номер блок-секции</label>        <input type="text" id="VAL_blockSection" name="VAL_blockSection" value={if $obj.ap_blockSection}'{$obj.ap_blockSection}'{else}''{/if}>    </div><!--/#name-wrap-->    <div id="firmName-wrap" class="slider">        <label for="firmName">Организация - продавец</label>        <input type="text" id="firmName" name="firmName" value={if $obj.firm_name}'{$obj.firm_name}'{else}''{/if}>    </div><!--/#name-wrap-->    <div id="firmName-wrap" class="slider">        <label for="VAL_roomNum">Количество комнат</label>        <input type="text" id="VAL_roomNum" name="VAL_roomNum" value={if $obj.ap_roomNum}'{$obj.ap_roomNum}'{else}''{/if}>    </div><!--/#name-wrap-->    <div id="firmName-wrap" class="slider" style = 'float: left;'>        <label for="VAL_area">Площадь</label>        <input type="text" id="VAL_area" name="VAL_area" value={if $obj.ap_area}'{$obj.ap_area}'{else}''{/if}>    </div>	<span style = 'margin-left: 2px; padding: 0px;'><small >M<sup>2</sup></small></span>    <div id="firmName-wrap" class="slider" >        <label for="VAL_floor">Этаж</label>        <input type="text" id="VAL_floor" name="VAL_floor" value={if $obj.ap_floor}'{$obj.ap_floor}'{else}''{/if}>    </div>	<div class="side-by-side clearfix">		<div>		<em>Отделка</em>        		<select data-placeholder="Отделка квартиры" class="chzn-select" id="VAL_finish" name="VAL_finish" style="width:350px;" tabindex="4" >			<option value=""></option> 			<option value="1" {if $obj.ap_finish == 1} selected {/if}>Черновая</option>							<option value="2" {if $obj.ap_finish == 2} selected {/if}>Получистовая</option>							<option value="3" {if $obj.ap_finish == 3} selected {/if}>Чистовая</option>						</select>		</div>	</div>	<div class="slider">	        <em>Описание квартиры</em>        	</div>	<div >	<div id="firmInfoDefault" onClick='editInfo();' title='Изменить информацию'>{if $obj.ap_infoShort}{$obj.ap_infoShort}{else}Добавить информацию{/if}</div>    <div id="objInfo" >		<div id="FCKeditor" style="display: none">			<input type="hidden" id="infoUpdt" 	name="infoUpdt" value="0">					<textarea id="firmInfo" name="firmInfo" cols="80" rows="20"></textarea>		</div>    </div><!--/#name-wrap-->	</div>    <div id="firmName-wrap" class="slider" style = 'float: left;'>        <label for="VAL_price">Цена</label>        <input type="text" id="VAL_price" name="VAL_price" value={if $obj.ap_price}'{$obj.ap_price}'{else}''{/if}>    </div>	<span style = 'margin-left: 2px; padding: 0px;'><small >тысяч рублей</small></span>    <div><input type="checkbox" id="VAL_sold" name="VAL_sold" style="float:left;" value='1' {if $obj.ap_sold}checked{/if}>  		<label for="VAL_sold"  style='float:none;' >Квартира продана</label></div>			{if $obj.img}	<div class='uploadedFiles'>	<em>Загруженные файлы</em>  		{section  name=img loop=$obj.img}		{if $obj.img[img].type != $obj.img[$smarty.section.img.index_prev].type || $smarty.section.img.first}			<div  class='fileGroup'><p>{if $obj.img[img].type ==1}Планировка{else}Фото{/if}</p>		{/if}		<div class=imgPad>		<div>		<img src='{$obj.img[img].src|replace:"size":"150"}' border=0> 		</div>        <div><input type="checkbox" id="delProp~{$obj.img[img].item_id}" name="delProp~{$obj.img[img].item_id}" style="float:left;" value='1'>  		<label for="delProp~{$obj.img[img].item_id}"  style='float:none;'>Удалить</label></div>		</div>		{if $obj.img[img].type != $obj.img[$smarty.section.img.index_next].type || $smarty.section.img.last}			</div >		{/if}		{/section}		</div>	{/if}		 <div class="side-by-side clearfix">		<div id=''></div>		<div class='slider-input'><div id="file_plan">	<noscript><p>Please enable JavaScript to use file uploader.</p></noscript> </div></div>		<div id='imgPrew_plan'></div>		<div id=''></div>		<div class='slider-input'><div id="file_foto">	<noscript><p>Please enable JavaScript to use file uploader.</p></noscript> </div></div>		<div id='imgPrew_foto'></div></div>	<div><input type="submit" id="btn" name="btn" value="Сохранить" ></div></form> <script type="text/javascript">var fileUploadParam = new Array();	var uploader = new Array();fileUploadParam[0]   = new createFileUploadParam('планировку квартиры', 'plan');fileUploadParam[1]   = new createFileUploadParam('фото квартиры', 'foto');{literal}/*var mark = document.getElementById('curMark');var model = document.getElementById('curModel');*/updateStylesApartments();{/literal}</script>